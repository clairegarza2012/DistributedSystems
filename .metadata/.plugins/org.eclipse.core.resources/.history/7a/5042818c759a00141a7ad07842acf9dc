package edu.neumont.csc380;

import com.hallaLib.HallaStor;

public class Store {

	private HallaStor store;
	
	private Store(){

	}
	
	public HallaStor getInstance(){
		
		if (store == null){
			synchronized (HallaStor.class){
				if (store == null){
					store = HallaStor.getInstance();

				}
			}
		}
		
		return store;
	}
	
}
