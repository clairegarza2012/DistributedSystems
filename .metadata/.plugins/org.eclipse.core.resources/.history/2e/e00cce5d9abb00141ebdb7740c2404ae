
public class Driver {

	public static void main(String[] args){
		
		Driver driver = new Driver();
	
		int first = 1;
		int second = 1;
		int n = 6;
		
		n = doSequenceInterative(first, second, n);
		
		System.out.println("N: " + n);
	}

	private static int doSequenceInterative(int first, int second, int n) {

		for (int count = 1; count < n; count++){
			second += first;
			first = second;
			System.out.println("Count: " + count + " n: " + second);
		}
		
		return second;
	}
	
	
	
	
}
